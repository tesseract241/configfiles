#! /bin/env bash
basename=$(basename "$0")
if [ $# -eq 0 ]; then
    echo -e "Usage: $basename [\033[1mquery\033[0m]"
    exit -1
fi
query=""
if [ $# -gt 1 ]; then
    query=$*
    query=${query// /+}
else
    query=$1
fi
LESSOPEN="||/home/tesseract/Coding/github_test/gitless %s" less "$query-1" "$query-2" "$query-3" "$query-4" "$query-5"
